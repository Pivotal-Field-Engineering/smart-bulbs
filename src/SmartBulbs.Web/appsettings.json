{
  "twitterSearch": "#cfbulbs",
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "CredHubClient": {
    "validateCertificates": false
  },
  "eureka": {
    "client": {
      "shouldFetchRegistry": false,
      "ValidateCertificates": false
    }
  },
  "hystrix": {
    "command": {
      "SetColor": {
        "execution": {
          "timeout": {
            "enabled": false
          }
        }
      }
    }
  },
  "management": {
    "endpoints": {
      "path": "/cloudfoundryapplication",
      "cloudfoundry": {
        "validateCertificates": false
      }
    }
  }
}
